{
  "name": "ilurnwitu-chatbot",
  "version": "1.0.0",
  "description": "A Node.js chatbot that learns from conversations over time.",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "node --watch src/server.js",
    "debug": "cross-env DEBUG=app:* LOG_LEVEL=debug node --watch src/server.js",
    "teach:english-math": "node scripts/teach-english-math.js --mode both",
    "teach:english-math:file": "node scripts/teach-english-math.js --mode file",
    "teach:english-math:api": "node scripts/teach-english-math.js --mode api",
    "teach:emoji-unicode": "node scripts/teach-emojis-unicode.js --mode both",
    "teach:emoji-unicode:file": "node scripts/teach-emojis-unicode.js --mode file",
    "teach:emoji-unicode:api": "node scripts/teach-emojis-unicode.js --mode api",
    "teach:hackclub": "node scripts/teach-hackclub-bulk.js --mode both",
    "teach:hackclub:file": "node scripts/teach-hackclub-bulk.js --mode file",
    "teach:hackclub:api": "node scripts/teach-hackclub-bulk.js --mode api"
  },
  "keywords": [
    "chatbot",
    "nodejs",
    "learning"
  ],
  "license": "MIT",
  "dependencies": {
    "cross-env": "^7.0.3",
    "debug": "^4.4.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jszip": "^3.10.1",
    "pdf-parse": "^1.1.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  }
}